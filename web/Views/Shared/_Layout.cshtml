<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title -Usuarios</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>



<body>

    <nav class="navbar navbar-light  ">
        <a class="navbar-brand" href="#">
            <img src="https://images.vexels.com/media/users/3/135251/isolated/lists/ab893f9074d536e3e940d61f0fc62b39-los-usuarios-firman-en-rojo.png" width="50" height="50" alt="">
            @Html.ActionLink("Usuarios", "Index", "Usuarios", new { area = "" }, new { @class = "navbar navbar-Brand navbar-light btn-success" })
        </a>
    </nav>
   

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Usuarios</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
